<?xml version="1.0" encoding="ISO-8859-1" ?>
<interface id="speclib_add">
    <download>
    </download>

    <!-- View definitions -->
    <view id="condensed_stats" label="condensed_stats">
        <blockRef id="main" type="condensed_stats"/>
    </view>

    <!-- Block definitions -->
    <block id="condensed_stats" type="table">
        <data>
            <source type="file" name="condensed_stats/"/>
            <parsers>
                <parser type="sortedTabular" sortBy="filename" operator="ascending"/>
            </parsers>

            <row>
                <column type="text" field="filename" label="filename" width="12"/>
            </row>
        </data>

    </block>
</interface>
